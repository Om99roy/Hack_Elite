      "routine": "#10B981",
      "normal": "#059669"
    }
  },
  "api_endpoints": {
    "authentication": [
      "POST /api/auth/register",
      "POST /api/auth/login",
      "POST /api/auth/biometric-verify",
      "POST /api/auth/otp-send",
      "POST /api/auth/otp-verify"
    ],
    "patient_management": [
      "GET /api/patient/profile",
      "PUT /api/patient/profile",
      "POST /api/patient/medical-history",
      "GET /api/patient/dashboard"
    ],
    "eye_test": [
      "POST /api/test/capture",
      "POST /api/test/upload",
      "GET /api/test/results/:id",
      "GET /api/test/history"
    ],
    "ai_analysis": [
      "POST /api/ai/analyze",
      "GET /api/ai/results/:id",
      "POST /api/ai/feedback"
    ],
    "appointments": [
      "GET /api/appointments/available",
      "POST /api/appointments/book",
      "PUT /api/appointments/:id",
      "DELETE /api/appointments/:id"
    ],
    "doctor_portal": [
      "GET /api/doctor/queue",
      "GET /api/doctor/patient/:id",
      "POST /api/doctor/diagnosis",
      "POST /api/doctor/prescription"
    ]
  },
  "database_schema": {
    "users": [
      "id", "email", "phone", "role", "created_at", "updated_at"
    ],
    "patients": [
      "user_id", "full_name", "dob", "gender", "emergency_contact", "biometric_template", "medical_history"
    ],
    "doctors": [
      "user_id", "license_number", "specialization", "hospital_affiliation", "verified"
    ],
    "eye_tests": [
      "id", "patient_id", "image_url", "ai_results", "risk_score", "priority_level", "created_at"
    ],
    "appointments": [
      "id", "patient_id", "doctor_id", "datetime", "type", "status", "notes"
    ],
    "prescriptions": [
      "id", "patient_id", "doctor_id", "medications", "instructions", "issued_date"
    ]
  },
  "deployment": {
    "frontend": "Vercel/Netlify",
    "backend": "Railway/Render",
    "database": "MongoDB Atlas/Supabase",
    "file_storage": "Cloudflare R2/AWS S3",
    "ai_models": "Hugging Face/Replicate",
    "monitoring": "Sentry/LogRocket"
  }
}
